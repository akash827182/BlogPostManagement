secrets.json

# Configuration files with local or sensitive info
appsettings.Development.json
appsettings.Local.json
appsettings.json
appsettings.*.json


# Entity Framework Core Migrations snapshots 
Migrations/*Snapshot.cs

# Rider/JetBrains files 
.idea/
*.DotSettings.user

# Other temp files
*.tmp
*.bak
*.swp

# OS-specific files
.DS_Store
Thumbs.db
ehthumbs.db
desktop.ini

# Templating & cache
_ReSharper*/
*.cache
